{
  "name": "tic-tac-toe-service",
  "version": "1.0.0",
  "description": "Tic-Tac-Toe Service",
  "scripts": {
    "test-int": "mocha -r ts-node/register 'src/tests/integration/**/*.ts'",
    "test-unit": "mocha -r ts-node/register 'src/tests/unit/**/*.ts'",
    "test": "mocha -r ts-node/register 'src/tests/**/*.ts'",
    "dev": "nodemon",
    "start": "tsc && node ./build/app.js",
    "lint": "eslint src/** --ext .ts",
    "validate": "tsc --noEmit",
    "docker-compose-build": "docker-compose --env-file .env.local.docker build",
    "docker-compose-up": "docker-compose --env-file .env.local.docker up"
  },
  "author": "Gustavo Sabel",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.14.0"
  },
  "dependencies": {
    "ajv": "^8.8.2",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "http-errors": "^1.8.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@swc/core": "^1.2.139",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.1",
    "@types/express-session": "^1.17.3",
    "@types/http-errors": "^1.8.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^15.14.2",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "axios": "^0.26.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "sinon": "^13.0.1",
    "supertest": "^6.2.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5"
  }
}
